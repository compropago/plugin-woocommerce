  "config": {
    "optimize-autoloader": true
  },
  "keywords": [
    "pagos internet",
    "payment gateway",
    "woocommerce",
    "compropago",
    "cash",
    "spei"
  ],
  "require": {
    "compropago/php-sdk": "5.0.0.0"
  },
  "require-dev": {
    "wp-cli/wp-cli-bundle": "^2.1"
  },
  "scripts": {
      "build": [
        "composer clean",
        "composer install",
        "composer pot",
        "rm -rf vendor",
        "composer install --no-dev",
        "zip -r compropago.zip . -x \"*.git*\" \"*.idea*\" \".DS_Store\" \"build.sh\"",
        "composer install"
      ],
      "clean": [
      "rm -rf vendor",
        "rm -rf languages",
        "rm -f compropago.zip",
        "find . -name '.DS_Store' -delete"
      ],
      "pot": "./vendor/wp-cli/wp-cli/bin/wp i18n make-pot . languages/compropago.pot"
  },
  "scripts-descriptions": {
      "build": "Generate file for Wordpress plugin",
      "clean": "Remove vendor, build and translations",
      "pot": "Create a POT file for the WordPress plugin/theme in the current directory"
  },
  "support": {
    "docs": "http://demo.compropago.com/plugins/woocommerce",
    "email": "soporte@compropago.com",
    "issues": "https://github.com/compropago/plugin-woocommerce/issues",
    "source": "https://github.com/compropago/plugin-woocommerce.git"
  }
}
